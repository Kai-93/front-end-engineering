{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "standard",
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": "off",
    "no-tabs": "off",
    "indent": "off",
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "no-debugger": 0,
    "eol-last": "off"
  }
}